
config dnsmasq 'dnsmasq'
	option domainneeded '1'
	option boguspriv '0'
	option filterwin2k '0'
	option localise_queries '1'
	option rebind_protection '0'
	option rebind_localhost '1'
	option local '/lan/'
	option domain 'lan'
	option expandhosts '1'
	option nonegcache '1'
	option cachesize '1000'
	option authoritative '1'
	option readethers '1'
	option leasefile '/tmp/dhcp.leases'
	option resolvfile '/tmp/resolv.conf.d/resolv.conf.auto'
	option nonwildcard '1'
	option localservice '1'
	option ednspacket_max '1232'
	option filter_aaaa '0'
	option filter_a '0'
	option noresolv '1'
	option localuse '1'
	option confdir '/etc/dnsmasq.d'

config dhcp 'lan'
	option interface 'lan'
	option start '100'
	option limit '101'
	option leasetime '12h'
	option dhcpv4 'server'
	option dhcpv6 'server'
	option ra 'server'
	option ra_slaac '1'
	list ra_flags 'managed-config'
	list ra_flags 'other-config'
	option ra_default '1'
	list dhcp_option '42,192.168.77.112'  # ntp-servers
	list dhcp_option '44,192.168.77.114'  # netbios-name-servers (WINS)
	list dhcp_option '45,192.168.77.114'  # netbios-dd-server
	list dhcp_option '46,8'               # netbios-node-type (8 = H-node)

config dhcp 'wan'
	option interface 'wan'
	option ignore '1'

config odhcpd 'odhcpd'
	option maindhcp '0'
	option leasefile '/tmp/hosts/odhcpd'
	option leasetrigger '/usr/sbin/odhcpd-update'
	option loglevel '4'

config host
	option name 'xps7590l1'
	option ip '192.168.69.19'
	option mac 'D4:81:D7:B9:35:59'
	option dns '1'
	option leasetime 'infinite'

config ipset 'vpn_m'
list domain 'main_nonexistent_domain'
option table_family 'inet'
	list name 'vpn4_m'
	list name 'vpn6_m'
	# list name 'tun2ray4'
	# list name 'tun2ray6'
	# list name 'tproxy4'
	# list name 'tproxy6'


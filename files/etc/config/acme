config acme 'main'
	option account_email 'r@REPLACE_secret_tld_REPLACE'
	option debug '0'

config cert 'supasta'
	option enabled '1'
	option use_staging '0'
	option keylength 'ec-384'
	option validation_method 'dns'
	option dns 'dns_cf'
	option acme_server 'letsencrypt'
	list domains 'REPLACE_secret_tld_REPLACE'
	list domains '*.REPLACE_secret_tld_REPLACE'
	list credentials 'CF_Zone_ID="REPLACE_clofa_zo_REPLACE"'
	list credentials 'CF_Token="REPLACE_clofa_to_REPLACE"'

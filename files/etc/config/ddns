config ddns 'global'
	option ddns_dateformat '%F %R'
	option ddns_loglines '250'
	option upd_privateip '1'
	option ddns_rundir '/var/run/ddns'
	option ddns_logdir '/var/log/ddns'

config service 'cloudflare'
	option service_name 'cloudflare.com-v4'
	option lookup_host 'REPLACE_hostname_REPLACE.REPLACE_secret_tld_REPLACE'
	option domain 'REPLACE_hostname_REPLACE@REPLACE_secret_tld_REPLACE'
	option username 'Bearer'
	option password 'REPLACE_clofa_to_REPLACE'
	option interface 'wan4'
	option ip_source 'network'
	option ip_network 'wan4'
	option enabled '1'
	option use_ipv6 '0'
	option use_syslog '1'
	option check_unit 'minutes'
	option force_unit 'hours'
	option retry_unit 'seconds'
	option use_logfile '0'
	option check_interval '11'
	option force_interval '33'
	option retry_interval '22'

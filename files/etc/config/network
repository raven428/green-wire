
config interface 'loopback'
	option device 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config globals 'globals'
	option ula_prefix 'fddc:1df3:9d1a::/48'

config device 'br_lan'
	option name 'br-lan'
	option type 'bridge'
	option macaddr '00:0c:29:be:03:b2'
	list ports 'lan1'
	list ports 'lan2'
	list ports 'lan3'
	list ports 'lan4'
	list ports 'sfp2'
	list ports 'eth1'
	list ports 'wan'

config interface 'lan'
	option device 'br-lan.77'
	option proto 'static'
	option ipaddr '192.168.69.222'
	option netmask '255.255.255.0'
	option ip6assign '60'

config device 'eth0'
	option name 'eth0'
	option macaddr '00:0c:29:be:03:b2'

config device 'eth1'
	option name 'eth1'
	option macaddr '00:0c:29:be:03:b2'

config device 'wan'
	option name 'wan'
	option macaddr '00:0c:29:be:03:b2'

config device 'lan1'
	option name 'lan1'
	option macaddr '00:0c:29:be:03:b2'

config device 'lan2'
	option name 'lan2'
	option macaddr '00:0c:29:be:03:b2'

config device 'lan3'
	option name 'lan3'
	option macaddr '00:0c:29:be:03:b2'

config device 'lan4'
	option name 'lan4'
	option macaddr '00:0c:29:be:03:b2'

config device 'sfp2'
	option name 'sfp2'
	option macaddr '00:0c:29:be:03:b2'

config interface 'wan4'
	option device 'br-lan.5'
	option proto 'dhcp'
	option peerdns '0'

config interface 'wan6'
	option device 'br-lan.5'
	option proto 'dhcpv6'
	option peerdns '0'

config bridge-vlan 'lan77'
	option device 'br-lan'
	option vlan '77'
	list ports 'lan1'
	list ports 'lan2'
	list ports 'lan3'
	list ports 'lan4'
	list ports 'sfp2'
	list ports 'wan:t' # lan tag on wan port

config bridge-vlan 'wan5'
	option device 'br-lan'
	option vlan '5'
	list ports 'wan'
	list ports 'eth1'

config interface 'tun2ray'
	option device 'tun2ray'
	option proto 'none'

config rule 'byp_ru4'
	option name 'byp'
	option mark '0x199'
	option priority '100'
	option lookup 'byp_m'

config route 'byp_ro4'
	option name 'byp'
	option interface 'wan4'
	option table 'byp_m'
	option target '0.0.0.0/0'

config rule6 'byp_ru6'
	option name 'byp'
	option mark '0x199'
	option priority '100'
	option lookup 'byp_m'

config route6 'byp_ro6'
	option name 'byp'
	option interface 'wan6'
	option table 'byp_m'
	option target '::/0'

config rule 'tun2ray_ru4'
	option name 'tun2ray'
	option mark '0x100'
	option priority '100'
	option lookup 'tun2ray'

config route 'tun2ray_ro4'
	option name 'tun2ray'
	option interface 'tun2ray'
	option table 'tun2ray'
	option target '0.0.0.0/0'

config rule6 'tun2ray_ru6'
	option name 'tun2ray'
	option mark '0x100'
	option priority '100'
	option lookup 'tun2ray'

config route6 'tun2ray_ro6'
	option name 'tun2ray'
	option interface 'tun2ray'
	option table 'tun2ray'
	option target '::/0'

config rule 'prskt_ru4'
	option name 'prskt'
	option mark '0x197'
	option priority '100'
	option lookup 'prskt'

config route 'prskt_ro4'
	option name 'prskt'
	option interface 'prskt'
	option table 'prskt'
	option target '0.0.0.0/0'

config interface 'docker'
	option device 'docker0'
	option proto 'none'
	option auto '0'

config device 'docker_bridge'
	option type 'bridge'
	option name 'docker0'

# msmtp relay config
account default
host 127.0.0.1
auth on
user REPLACE_SMTP_login_REPLACE
password REPLACE_SMTP_passwd_REPLACE
domain REPLACE_hostname_REPLACE.REPLACE_secret_tld_REPLACE
port 11025
tls on
tls_starttls on
tls_certcheck off
from %U@REPLACE_hostname_REPLACE.REPLACE_secret_tld_REPLACE
allow_from_override on
set_from_header on
syslog LOG_MAIL
